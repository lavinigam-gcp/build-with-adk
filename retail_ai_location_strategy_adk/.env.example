# Retail AI Location Strategy - Environment Configuration
# Copy this file to .env and fill in your actual values

# Google AI Studio API Key (Required)
# Get from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# IMPORTANT: Set this to FALSE to use AI Studio instead of Vertex AI
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Google Maps API Key (Required for competitor mapping)
# Get from: https://console.cloud.google.com/apis/credentials
# Enable "Places API" for your project
MAPS_API_KEY=your_maps_api_key_here
